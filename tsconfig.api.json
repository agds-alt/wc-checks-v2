{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "noEmit": false,
    "outDir": ".vercel/output",
    "rootDir": ".",
    "skipLibCheck": true
  },
  "include": ["api/**/*"],
  "exclude": ["node_modules"]
}
