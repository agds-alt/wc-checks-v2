{
  "buildCommand": "pnpm run build",
  "installCommand": "corepack enable && corepack prepare pnpm@9.15.0 --activate && pnpm install",
  "framework": "nextjs",
  "headers": [
    {
      "source": "/_next/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
